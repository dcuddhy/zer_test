<%= form_for @event do |f| %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.label :start %>
  <%= f.time_field :start %>

  <%= f.label :end %>
  <%= f.time_field :end %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :track %>
  <%= f.text_field :track %>

  <%= f.submit %>

<% end %>
